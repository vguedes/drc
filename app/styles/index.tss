	height: Titanium.UI.FILL,
	borderColor: '#92',
	borderWidth: 1, height: 190
}

".content_icon_view": {
	width: Titanium.UI.FILL,
	height: 150
}

".item_icon": {
	width: "50%",
	top: "20px",
}

".item_icon_label": {
	top: "20px",
	color: "#6E6F71",
	width: Titanium.UI.SIZE,
	height: Titanium.UI.SIZE,
	font: {
		fontSize: 17
	},
	height:40,
	textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
}

".content_label_view": {
	width: Titanium.UI.FILL,
	height: 40,
	backgroundColor: "#6E6F71",
