/* HEADER STYLE*/
".container": {
	backgroundColor:"#EEEEEE",
	width: Ti.UI.FILL,
	height: Titanium.UI.FILL,
	fullscreen: false,
}

".title": {
	width: Titanium.UI.FILL,
	height: 58,
 	backgroundColor:"#5090cd",
	top: 0
}

".menu_btn": {
	left: 10,
	width: 18,
	height: 18
}

".label_title": {
	width: Titanium.UI.SIZE,
	height: Titanium.UI.FILL,
 	color: "white",
 	font: {fontWeight:'bold',fontSize: 18},
	left: 60
}

".bell_btn": {
	right: 48,
	width: 16,
	height: 16,
}

".find_btn": {
	right: 11,
	width: '48px',
	height: '48px'
}
/* HEADER STYLE*/

".apponitment_btn": {
	width: "90%",
	height: 65,
	top: "15px",
	backgroundColor: "#E7B00F",
	color: "white",
